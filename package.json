{
  "name": "praatio",
  "version": "2.1.4",
  "description": "A javascript library for working with praat, textgrids, time aligned audio transcripts, and audio files.",
  "scripts": {
    "linter": "eslint --ignore-path .gitignore ./lib",
    "test": "jest --config=./test/unit/jest.conf.js",
    "coveralls": "cat ./test/unit/coverage/lcov.info | coveralls",
    "docs": "jsdoc -d ./docs --readme ./README.md ./lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timmahrt/praatIO.js.git"
  },
  "keywords": [
    "praat",
    "textgrid",
    "transcript",
    "speech",
    "audio"
  ],
  "author": "Tim Mahrt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/timmahrt/praatIO.js/issues"
  },
  "main": "/lib/index.js",
  "files": [
    "/LICENSE",
    "/README.md",
    "/lib"
  ],
  "homepage": "https://github.com/timmahrt/praatIO.js#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.4.5",
    "babel-jest": "^24.8.0",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.8.0",
    "jest-matcher-deep-close-to": "^1.3.0",
    "jsdoc": "^3.6.2"
  },
  "dependencies": {
    "iconv-lite": "^0.4.24"
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "@babel/preset-env"
        ]
      }
    }
  }
}
